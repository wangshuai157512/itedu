.mark_title_img image {
  /* width: 40rpx;
  height: 40rpx; */
  height: 64rpx;
line-height: 64rpx;
padding-bottom: 4rpx;
/* box-sizing: border-box; */

}
.mark_title_one {
  width: 128rpx;
  margin-right: 34rpx;
}
.mark_title_img image {
  width: 40rpx;
  height: 40rpx;
  margin-right: 34rpx;
  vertical-align: middle;
}
.mark_title_good {
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(149,156,166,1);
}
.mark .mark_form{
  width: 690rpx;
  height: 800px;
}
.mark form textarea {
  width: 690rpx;
  height: 336rpx;
  border-radius:8px;
  border:1px solid rgba(184,192,204,1);
  padding-top: 22rpx;
  box-sizing: border-box;
  margin-top: 23rpx;
  margin-bottom: 50rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:#2E3033;
  padding-left: 36rpx;
  /* text-indent: 56rpx */
}
