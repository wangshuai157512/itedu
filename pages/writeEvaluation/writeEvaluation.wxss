.mark form .btn {
  width:380rpx;
  height:88rpx;
  background:rgba(184,192,204,1);
  color:#fff;
  box-shadow:0px 5px 14rpx 0px rgba(217,225,236,1);
  border-radius:44rpx;
}
.mark form .active {
  background-color: #1B6FE3 !important;
}