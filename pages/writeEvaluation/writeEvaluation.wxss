.mark {
  width: 690rpx;
  margin: 0 30rpx;
  box-sizing: border-box;
}
.mark_title {
  width: 690rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:700;
  color:rgba(46,48,51,1);
}
