}
.evaluation_head_img image {
  width: 100%;
  height: 100%;
}
.goevaluation {
  display: flex;
  justify-content: center;
  padding-left: 30rpx;
  box-sizing: border-box;
  margin: 40rpx 0;
}
.goevaluation view {
  width:186rpx;
  height:60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius:30rpx;
  border:1px solid rgba(27,111,227,1);
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(27,111,227,1);
}
.evaluation {
  width: 750rpx;
  padding: 40rpx 58rpx 10rpx 30rpx;
  box-sizing: border-box;
  /* background-color: #ccc; */
}
.evaluation_top {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(149,156,166,1);
}
.evaluation_top_left image {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.evaluation_top_left text {
  margin-left: 20rpx;
}
.evaluation_top_right image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  padding-bottom: 5rpx;
  box-sizing: border-box;
}
.evaluation_top_right text {
  margin-right: 10rpx;
}

.evaluation_bottom {
  margin-left: 84rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(46,48,51,1);
}

