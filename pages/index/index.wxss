.freeClass {
  font-family:PingFangSC-Medium,PingFang SC;
  padding-bottom: 4rpx;
  position: relative;
}
.rectangle {
  width:148rpx;
  height:14rpx;
  background:rgba(120,207,255,1);
  border-radius:7rpx;
  opacity:0.39;
  position: absolute;
  bottom: 4rpx;
}
.freeClass_show {
  display: flex;
  margin-top: 28rpx;
  position: relative;
}
.freeClass_show_btn{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 99;
}
.freeClass_show_btn button {
  width: 100%;
  height: 100%;
  padding: 0;
  opacity:0;
}
.freeClass_show .freeClass_left {
  width:146rpx;
  height:146rpx;
  /* opacity:0.4; */
}
.freeClass_show .freeClass_left image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}
.freeClass_show .freeClass_right {
  position: relative;
  top: -8rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  flex: 1;
}
.explain {
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(96,101,108,1);
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.watchPeople {
  width: 100;
  font-size:22rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(184,192,204,1);
  position: absolute;
  bottom: -12rpx;
}
.excellent_content {
  margin-left: 30rpx;
}
.excellentClass {
  font-size:36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:700;
  padding: 51rpx 0 30rpx 0;
  position: relative;
}
.excellentClass .rectangle {
  bottom: 30rpx;
}

.excellentClass_show {
  width: 335rpx;
  display: inline-block;
  box-shadow:0px 2px 8px 0px rgba(183,183,183,0.5);
  border-radius:10rpx;
  margin: 0 20rpx 20rpx 0;
  overflow: hidden;
  position: relative;
}
.excellentClass_show_btn {
  width: 335rpx;
  height: 300rpx;
  position: absolute;
  left: 0;
  top: 0;
  opacity: 0;
}
.excellentClass_show_btn button {
  width: 100%;
  height: 100%;
  padding: 0;
}
/* .excellentClass_left {
  margin-right: 20rpx;
} */
.excellentClass_left_img {
  width:335rpx;
  height:188rpx;
}
.excellentClass_left_img image {
  width: 100%;
  height: 100%;
}
.excellentClass_show_explain {
  height: 114rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  color:rgba(46,48,51,1);
  padding: 20rpx 16rpx;
  box-sizing: border-box;
  position: relative;
}
.excellentClass_show_explain_title {
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}

.excellentClass_show_explain .watchPeople {
  bottom: 20rpx;
}

.collection-box{
  position: fixed;
  /* top: 124rpx; */
  right: 48px;
  z-index: 999;
  background: rgba(0,0,0,.5);
  width: 190px;
  height: 30px;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 100px;
}
.collection-box .collection-text{
  color: #fff;
  font-size: 12px;
}
.collection-box image {
  width: 18px;
  height: 18px;
}
.collection-box:after {
  display:block;
  content:'';
  position: absolute;
  width: 0;
  height: 0;
  border-top: 7px solid transparent;
  border-bottom: 7px solid rgba(0,0,0,0.5);
  border-left: 7px solid transparent;
  border-right: 7px solid transparent;
  top:-14px;
  right: 36rpx;
  transform: translateX(-50%);
}
